{
  "schemaVersion": 1,
  "id": "colorfulcommands",
  "version": "${version}",
  "name": "Colorful Commands",
  "description": "Configure your command highlighting colors.",
  "authors": [
    "TehBrian"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mods/ColorfulCommands",
    "sources": "https://github.com/TehBrian/ColorfulCommands",
    "issues": "https://github.com/TehBrian/ColorfulCommands/issues"
  },
  "license": "MIT",
  "icon": "assets/colorfulcommands/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "dev.tehbrian.colorfulcommands.ColorfulCommands"
    ],
    "modmenu": [
      "dev.tehbrian.colorfulcommands.compat.ModMenuCompat"
    ]
  },
  "mixins": [
    "colorfulcommands.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://thbn.me/discord"
      }
    }
  }
}
